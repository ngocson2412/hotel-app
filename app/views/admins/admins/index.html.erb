<div class="">
    <div class="row">
        <div class="col-md-12 col-sm-12 ">
            <div class="x_panel">
                <div class="x_title">
                    <h2>Customers</h2>
                    <%= link_to "Add Customer", new_admins_admin_path, :class => "btn btn-success btn-sm", :style => "margin-left: 15px;" %>
                    <ul class="nav navbar-right panel_toolbox">
                        <li><a class="collapse-link"><i class="fa fa-chevron-up"></i></a>
                        </li>
                        <li class="dropdown">
                            <a href="#" class="dropdown-toggle" data-toggle="dropdown" role="button" aria-expanded="false"><i class="fa fa-wrench"></i></a>
                            <div class="dropdown-menu" aria-labelledby="dropdownMenuButton">
                                <a class="dropdown-item" href="#">Settings 1</a>
                                <a class="dropdown-item" href="#">Settings 2</a>
                            </div>
                        </li>
                        <li><a class="close-link"><i class="fa fa-close"></i></a>
                        </li>
                    </ul>
                    <div class="clearfix"></div>
                </div>
                <div class="x_content">
                    <div class="row">
                        <div class="col-sm-12">
                            <div class="card-box table-responsive">
                                <table id="datatable-checkbox" class="table table-striped table-bordered bulk_action" style="width:100%">
                                    <thead>
                                        <tr>
                                            <th>
                                                <input type="checkbox" id="check-all">
                                            </th>
                                            <th>ID</th>
                                            <th>User</th>
                                            <th>User Name</th>
                                            <th>Phone</th>
                                            <th>Address</th>
                                            <th>Created at</th>
                                            <th>Updated at</th>
                                            <th>Action</th>
                                        </tr>
                                    </thead>
                                    <tbody>
                                        <% @admins.each do |admin| %>
                                            <tr>
                                                <td>
                                                    <input type="checkbox" id="check-all">
                                                </td>
                                                <td><%= admin.id %></td>
                                                <th><%= link_to admin.name, admins_admin_path(admin) %></th>
                                                <td>123456789</td>
                                                <td>123456789</td>
                                                <td>HaNoi Hoang Mai ABC123</td>
                                                <td><%= admin.created_at%></td>
                                                <td><%= admin.updated_at%></td>
                                                <td>
                                                    <%= link_to admins_admin_path(admin), :class => "btn btn-primary btn-sm" do %>
                                                        <i class="fa fa-edit"> </i> Show
                                                    <% end%>
                                                </td>
                                            </tr>
                                        <% end %>
                                    </tbody>
                                </table>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
